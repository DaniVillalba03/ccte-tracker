# Ground Station - CCTE Tracker Environment Variables

# Vite Configuration
VITE_APP_NAME="Ground Station - CCTE Tracker"
VITE_APP_VERSION="1.0.0"

# Serial Port Configuration
VITE_DEFAULT_BAUD_RATE=115200
VITE_SERIAL_BUFFER_SIZE=1024

# IndexedDB Configuration
VITE_DB_NAME="RocketMissionDB"
VITE_DB_VERSION=1
VITE_BATCH_SIZE=100

# Map Configuration
VITE_MAP_TILES_URL="/maps/{z}/{x}/{y}.png"
VITE_MAP_CENTER_LAT=-33.4489
VITE_MAP_CENTER_LNG=-70.6693
VITE_MAP_ZOOM=13
VITE_MAX_TRAJECTORY_POINTS=1000

# Data Configuration
VITE_TELEMETRY_RATE_HZ=400
VITE_MAX_STORED_SAMPLES=100000

# Development
VITE_ENABLE_DEBUG_MODE=false
VITE_MOCK_SERIAL_DATA=false
